	gpiok: gpio@542e0000 {
		status = "okay";
		ngpios = <8>;
		gpio-ranges = <&pinctrl 0 160 8>;
	};
};

&pinctrl_z {
	gpioz: gpio@56200000 {
		status = "okay";
		ngpios = <10>;
		gpio-ranges = <&pinctrl_z 0 400 10>;
	};
};
